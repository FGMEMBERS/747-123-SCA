<?xml version="1.0"?>
<PropertyList>

<path>altimeter.ac</path>

 <animation>
  <type>scale</type>
  <x-offset>0.916955</x-offset>
  <y-offset>0.916955</y-offset>
  <z-offset>0.916955</z-offset>
 </animation>

<animation>
      <type>material</type>
      <object-name>AltFace</object-name>
      <object-name>ALTneedle</object-name>
      <object-name>altnum10000</object-name>
      <object-name>altnum1000</object-name>
      <object-name>altnum100</object-name>
      <object-name>mb01</object-name>
      <object-name>mb02</object-name>
      <object-name>mb03</object-name>
      <object-name>mb04</object-name>
      <object-name>inhg01</object-name>
      <object-name>inhg02</object-name>
      <object-name>inhg03</object-name>
      <object-name>inhg04</object-name>
      <emission>
            <red>0.3</red>
            <green>0.25</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
      </emission>
</animation>

<animation>
      <type>material</type>
      <object-name>casco</object-name>
      <object-name>parafusos</object-name>
      <object-name>BaroKnob</object-name>
      <emission>
            <red>0.3</red>
            <green>0.25</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/panel-norm[0]</factor-prop>
      </emission>
</animation>  

<!--####    InHg    ####-->

    <animation>
        <type>textranslate</type>
        <object-name>inhg01</object-name>
        <property>/instrumentation/altimeter/setting-inhg</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>inhg02</object-name>
        <property>/instrumentation/altimeter/setting-inhg</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>inhg03</object-name>
        <property>/instrumentation/altimeter/setting-inhg</property>
        <factor>1.0</factor>
        <step>0.1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>inhg04</object-name>
        <property>/instrumentation/altimeter/setting-inhg</property>
        <factor>10.0</factor>
        <step>0.01</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>mb01</object-name>
        <property>/instrumentation/altimeter/millibars</property>
        <factor>0.0001</factor>
        <step>1000</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>mb02</object-name>
        <property>/instrumentation/altimeter/millibars</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>mb03</object-name>
        <property>/instrumentation/altimeter/millibars</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>mb04</object-name>
        <property>/instrumentation/altimeter/millibars</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

<!--####    altimeter    ####-->

    <animation>
        <type>textranslate</type>
        <object-name>altnum100</object-name>
        <property>/instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.001</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>altnum1000</object-name>
        <property>/instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.0001</factor>
        <step>1000</step>
        <scroll>20</scroll>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>altnum10000</object-name>
        <property>/instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.00001</factor>
        <step>10000</step>
        <scroll>20</scroll>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ALTneedle</object-name>
        <property>/instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.36</factor>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>BaroKnob</object-name>
        <property>/instrumentation/altimeter/setting-inhg</property>
        <factor>-1600</factor>
        <center>
            <x-m>0.008615</x-m>
            <y-m>0.027116</y-m>
            <z-m>-0.028007</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

</PropertyList>